// list
let l: isList = List(3, 4, 5);
println(l);
println("size: " + size(l));
println("l[1]: " + l[1]);
println(set(l, 1, 10));
println("isEmpty: " + isEmpty(l));
println("isEmpty List(): " + isEmpty(List()));
println();

// string
let s: isString = "Haba Baba!";
println(s);
println("size: " + size(s));
println("s[0]: " + s[0]);
println(set(s, 0, 'B'));
println("isEmpty: " + isEmpty(s));
println("isEmpty '': " + isEmpty(""));
println();

// map
let m: isMap = Map(
    0,       1,
    "a",     2,
    List(0), '_'
);
println(m);
println(size(m));
println(isEmpty(m));
println("m[List(0)]: " + m[List(0)]);
println("m[0] = 10: " + set(m, 0, 10));
println();