// list
let l: isList = [3, 4, 5];
assert(toString(l) == "[3, 4, 5]");
assert(size(l) == 3);
assert(l[1] == 4);
assert(set(l, 1, 10) == [3, 10, 5]);
assert(isEmpty(l) == false);
assert(isEmpty([]));

// string
let s: isString = "Haba Baba!";
assert(size(s) == 10);
assert(s[0] == 'H');
assert(set(s, 0, 'B') == "Baba Baba!");
assert(isEmpty(s) == false);
assert(isEmpty(""));

// map
let m: isMap = Map(
    0,   1,
    "a", 2,
    [0], '_'
);
assert(toString(m) == "{0: 1, a: 2, [0]: _}");
assert(size(m) == 3);
assert(isEmpty(m) == false);
assert(isEmpty(Map()));
assert(m[[0]] == '_');
assert(set(m, 0, 10) == Map(
    0,   10,  
    "a", 2,  
    [0], '_')
);