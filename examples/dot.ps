interface infixQuadratic;
function infixQuadratic(x: isReal): isReal {
    x ** 2 + x + 1
}

interface chainedQuadratic;
function chainedQuadratic(x: isReal): isReal {
    let inc: isFn = (n) => n + 1;

    x
    .pow($this, 2)   // chained function call
    .(x + $this)     // chained expression
    .inc             // chained function call without args
}

let x: isReal = 3.5;
assert(
    infixQuadratic(x) 
    == chainedQuadratic(x)
);