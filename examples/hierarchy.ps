interface isEven;

// define new predicate
function isEven(x: isInt): isBool {
    x % 2 == 0
}

// all isEvens are valid isInts
derive(isInt, isEven);

let i: isInt  = 1;
let e: isEven = as(isEven, 4);

// + defined for [isInt, isInt] -> isInt
// called for [isInt, isEven]
let sum: isInt = i + e;
println(sum);